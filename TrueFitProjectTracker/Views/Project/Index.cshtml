@{
    string ProjectName = "Project X";
    
    Layout = "~/Views/Shared/Layouts/_Project.cshtml";
    ViewBag.Title = ProjectName;
}
@section Scripts {
<script src="//cdnjs.cloudflare.com/ajax/libs/highcharts/3.0.9/highcharts.js"></script>
<script>
    $(function () {
        $('#container').highcharts({
            chart: {
                backgroundColor: '#77E38C',
                borderRadius: 0
            },
            colors: ['#FFFFFF'],
            credits: {
                enabled: false
            },
            exporting: {
                enabled: false
            },
            title: {
                text: null
            },
            xAxis: {
                categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun',
                    'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
                lineColor: '#000000',
                tickColor: '#000000',
            },
            yAxis: {
                gridLineColor: '#000000',
                gridLineDashStyle: 'LongDash',
                title: {
                    style: {
                        color: '#FFFFFF'
                    },
                    text: 'Remaining Hours'
                }
            },
            legend: {
                layout: 'vertical',
                align: 'right',
                verticalAlign: 'middle',
                borderWidth: 0
            },
            series: [{
                name: 'Tokyo',
                data: [7.0, 6.9, 9.5, 14.5, 18.2, 21.5, 25.2, 26.5, 23.3, 18.3, 13.9, 9.6]
            }]
        });

        $('#container1').highcharts({
            chart: {
                backgroundColor: '#00D2FF',
                borderRadius: 0
            },
            colors: ['#FFFFFF'],
            credits: {
                enabled: false
            },
            exporting: {
                enabled: false
            },
            title: {
                text: null
            },
            xAxis: {
                categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun',
                    'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
                lineColor: '#000000',
                tickColor: '#000000',
            },
            yAxis: {
                gridLineColor: '#000000',
                gridLineDashStyle: 'LongDash',
                title: {
                    style: {
                        color: '#FFFFFF'
                    },
                    text: 'Remaining Hours'
                }
            },
            legend: {
                layout: 'vertical',
                align: 'right',
                verticalAlign: 'middle',
                borderWidth: 0
            },
            series: [{
                name: 'Tokyo',
                data: [7.0, 6.9, 9.5, 14.5, 18.2, 21.5, 25.2, 26.5, 23.3, 18.3, 13.9, 9.6]
            }]
        });
    });
</script>
}
@section ProgressBar {
<div class="progress">
<div class="progress-bar progress-bar-info" aria-valuemax="100" aria-valuemin="0" aria-valuenow="20" role="progressbar" style="width: 20%">
<span>20%</span>
</div>
</div>
}
<section class="content no-border row">
<div class="col-md-6">
<article class="task-burndown">
<div class="panel panel-default">
<div class="panel-heading">Panel heading without title</div>
<div class="panel-body">
<div id="container"></div>
<div id="daily-handlers"><p>More exciting things and stuff like that!</p></div>
</div>
</div>
</article>
</div>

<div class="col-md-6">
<article class="bug-burndown">
